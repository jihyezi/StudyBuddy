{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },  
    { "src": "public/**", "use": "@vercel/static" } 
  ],
  "rewrites": [
    { 
      "source": "/api/searchBook",
      "destination": "/api/searchBook.js"
    },
    {
      "source": "/api/searchPlace", 
      "destination": "/api/searchPlace.js"
    }
  ] 
}