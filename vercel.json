{
    "rewrites": [
      {
        "source": "/api/searchBook",
        "destination": "https://openapi.naver.com/v1/search/book.json"
      },
      {
        "source": "/api/searchPlace",
        "destination": "https://openapi.naver.com/v1/search/local.json"
      }
    ],
    "headers": [
      {
        "source": "/api/searchBook",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate"
          }
        ]
      },
      {
        "source": "/api/searchPlace",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate"
          }
        ]
      }
    ]
  }
  